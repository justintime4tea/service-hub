{
  "source": ".",
  "destination": "./docs/esdoc",
  "excludes": [
    ".external-ecmascript.js",
    "node_modules",
    "(spec|Spec|test|Test)\\.js$",
    "coverage",
    "docs",
    "test"
  ],
  "plugins": [
    {
      "name": "esdoc-standard-plugin",
      "option": {
        "lint": {
          "enable": true
        },
        "coverage": {
          "enable": true
        },
        "accessor": {
          "access": [
            "public",
            "protected",
            "private"
          ],
          "autoPrivate": true
        },
        "undocumentIdentifier": {
          "enable": false
        },
        "unexportedIdentifier": {
          "enable": true
        },
        "typeInference": {
          "enable": false
        },
        "test": {
          "source": "./",
          "interfaces": [
            "describe",
            "it",
            "context",
            "suite",
            "test"
          ],
          "includes": [
            "(spec|Spec|test|Test)\\.js$"
          ],
          "excludes": [
            "node_modules"
          ]
        }
      }
    }
  ]
}
