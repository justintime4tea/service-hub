{
  "tags": {
    "allowUnknownTags": true,
    "dictionaries": ["jsdoc"]
  },
  "source": {
    "include": ["index.js", "lib", "package.json", "README.md"],
    "includePattern": ".js$",
    "excludePattern": "(node_modules/|docs|test/|spec.js|Spec.js|test.js|Test.js)"
  },
  "plugins": [
    "plugins/markdown"
  ],
  "markdown": {
    "idInHeadings": true
  },
  "templates": {
    "cleverLinks": false,
    "monospaceLinks": true,
    "useLongnameInNav": false,
    "showInheritedInNav": true,
    "default": {
      "outputSourceFiles": true
    }
  },
  "opts": {
    "destination": "./docs/jsdoc",
    "encoding": "utf8",
    "private": true,
    "recurse": true,
    "template": "./node_modules/postman-jsdoc-theme"
  }
}
